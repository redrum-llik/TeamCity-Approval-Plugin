<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>approval-plugin</name>
        <display-name>Approvals</display-name>
        <version>@Version@</version> <!-- will be replaced from script -->
        <vendor>
            <name>JetBrains</name>  <!-- will be replaced from script -->
            <url>https://jetbrains.com</url> <!-- will be replaced from script -->
        </vendor>
        <description>Define builds that require approval from sufficiently permissioned users</description>
    </info>
    <deployment use-separate-classloader="true" allow-runtime-reload="true" node-responsibilities-aware="true"/>
</teamcity-plugin>